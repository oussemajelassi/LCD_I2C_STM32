#ifndef INC_LCD_I2C_H_
#define INC_LCD_I2C_H_

typedef enum
{
	LCD_I2C_OKAY,
	LCD_I2C_FAIL
}LCD_I2C_STATE;


class LCD_I2C
{
	char * LCD_I2C_FIRST_LINE  ;
	char * LCD_I2C_SECOND_LINE ;
	char * LCD_I2C_THIRD_LINE  ;
	char * LCD_I2C_FOURTH_LINE ;

	LCD_I2C_STATE LCD_I2C_INIT (void ) ;

};



#endif /* INC_LCD_I2C_H_ */
